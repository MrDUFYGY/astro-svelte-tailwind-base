{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "preserve",
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["astro/client"]
  },
  "include": [
    ".astro/types.d.ts",
    "src/**/*",
    "src/**/*.astro",
    "src/**/*.js",
    "src/**/*.ts",
    "src/**/*.jsx",
    "src/**/*.tsx",
    "src/**/*.svelte"
  ],
  "exclude": ["node_modules", "dist"]
}
